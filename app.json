{
  "expo": {
    "name": "GA RUN",
    "slug": "GA-RUN",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4D6BFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.garun",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "Aplikace používá polohu ke sledování běhu",
        "NSLocationWhenInUseUsageDescription": "Aplikace používá polohu ke sledování běhu",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Aplikace používá polohu ke sledování běhu",

        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "package": "com.garun",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4D6BFF"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
